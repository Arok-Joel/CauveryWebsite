  "buildCommand": "bun run vercel-build",
  "devCommand": "bun run dev",
  "installCommand": "bun install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/bun"
    }
  ],
  "env": {
    "PRISMA_SKIP_POSTINSTALL": "1"
  }
} 